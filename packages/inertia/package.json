{
  "name": "@inertiajs/inertia",
  "version": "0.8.6",
  "license": "MIT",
  "description": "A framework for creating server-driven single page apps.",
  "contributors": [
    "Jonathan Reinink <jonathan@reinink.ca>",
    "Sebastian De Deyne <sebastiandedeyne@gmail.com>"
  ],
  "homepage": "https://inertiajs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiajs/inertia.git",
    "directory": "packages/inertia"
  },
  "bugs": {
    "url": "https://github.com/inertiajs/inertia/issues"
  },
  "source": "src/index.js",
  "main": "dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:bundler && npm run build:browser",
    "build:bundler": "microbundle --format es,cjs",
    "build:browser": "microbundle --format modern,umd --name Inertia --external none",
    "watch": "microbundle watch --format cjs"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "deepmerge": "^4.0.0",
    "qs": "^6.9.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "microbundle": "^0.12.0"
  }
}
